{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:import/recommended",
    "plugin:import/typescript"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "import/order": [
      1,
      {
        "groups": [
          "external",
          "builtin",
          "internal",
          "sibling",
          "parent",
          "index"
        ]
      }
    ],
    "import/newline-after-import": ["error", { "count": 1 }]
  },
  "plugins": ["import"]
}
